<!DOCTYPE html>
<html>
<head>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
   
    <script type="text/javascript">
    function preventBack() { window.history.forward(); }
    setTimeout("preventBack()", 0);
    window.onunload = function () { null };
     </script>
</head>
<body >
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Remedy Acknowledgment", "HomePage", "Remedy", new { area = "10" }, new { @class = "navbar-brand" })

            </div>
            <div style="float:right">
                @Html.ActionLink("Logout", "HomePage", "Remedy", new { area = "10" }, new { @class = "navbar-brand" })
            </div>
        </div>
    </div>
     
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer >
            <p>&copy;2022-Remedy Acknowledgement</p>
        </footer>
        <footer style="text-align:right">
            <p>@DateTime.Now.Date,@DateTime.Now.DayOfWeek -@DateTime.Now.TimeOfDay </p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
